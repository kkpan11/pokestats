{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/app/*": ["src/app/*"],
      "@/animations": ["src/animations"],
      "@/animations/*": ["src/animations/*"],
      "@/BaseStyles": ["src/components/BaseStyles"],
      "@/BaseStyles/*": ["src/components/BaseStyles/*"],
      "@/client": ["src/components/client"],
      "@/components": ["src/components"],
      "@/components/*": ["src/components/*"],
      "@/constants": ["src/constants"],
      "@/constants/*": ["src/constants/*"],
      "@/MuiTheme": ["src/theme"],
      "@/MuiTheme/*": ["src/theme/*"],
      "@/helpers": ["src/helpers"],
      "@/helpers/*": ["src/helpers/*"],
      "@/types": ["types"],
      "@/types/*": ["types/*"],
      "@/PageComponents": ["src/components/pages"],
      "@/pages": ["src/old.pages"],
      "@/pages/*": ["src/old.pages/*"],
      "@/hooks": ["src/hooks"],
      "@/hooks/*": ["src/hooks/*"],
      "@/services": ["src/services"],
      "@/services/*": ["src/services/*"],
      "@/skeletons": ["src/components/skeletons"],
      "@/context": ["src/context"],
      "@/context/*": ["src/context/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "types/*.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/pages/ItemListPage/ItemTable"
  ],
  "exclude": ["node_modules", "dist", "src/**/*.js", "src/**/*.jsx"]
}
